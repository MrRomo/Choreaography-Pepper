    def mail(self, email_user, to, subject, text, attach, email_pwd, smtp, port):
        msg = email.MIMEMultipart.MIMEMultipart()
        msg['From'] = email_user
        msg['To'] = to
        msg['Subject'] = subject

        msg.attach(email.MIMEText.MIMEText(text, "html"))

        if attach:
            part = email.MIMEBase.MIMEBase('application', 'octet-stream')
            part.set_payload(open(attach, 'rb').read())
            email.Encoders.encode_base64(part)
            part.add_header('Content-Disposition','attachment; filename="%s"' % os.path.basename(attach))
            msg.attach(part)

        if( port != "" ):
            mailServer = smtplib.SMTP(smtp, port)
        else:
            mailServer = smtplib.SMTP(smtp)
        mailServer.ehlo()
        mailServer.starttls()
        mailServer.ehlo()
        mailServer.login(email_user, email_pwd)
        mailServer.sendmail(email_user, to, msg.as_string())

        mailServer.close()]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="input" type="3" type_size="1" nature="1" inner="0" tooltip="" id="2" /><Output name="output" type="1" type_size="1" nature="2" inner="0" tooltip="" id="3" /><Output name="onError" type="3" type_size="1" nature="2" inner="0" tooltip="" id="4" /></Box><Box name="Halago" id="34" localization="8" tooltip="Say some text with animations. The text can be localized." x="561" y="83"><bitmap>media/images/box/interaction/say.png</bitmap><script language="4"><content><![CDATA[import time

