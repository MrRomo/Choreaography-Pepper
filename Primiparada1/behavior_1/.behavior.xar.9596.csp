class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.autonomouslife = ALProxy("ALAutonomousLife")
        self.motion = ALProxy("ALMotion")
        self._awareness = ALProxy("ALBasicAwareness")
        self.motion.setExternalCollisionProtectionEnabled("Arms", False)

    def onLoad(self):
        self.joints = ["RElbowRoll","RElbowYaw","RHand","RShoulderPitch","RShoulderRoll","RWristYaw", "HeadPitch", "HeadYaw", "HipPitch"]
        self.fAngles = [0.7225048542022705, 0.1963496208190918, 0.753954291343689, 0.0490872859954834, -0.20862150192260742, 1.3575481176376343, 0.0, 0.0, 0.0]
        self.movedJoint = ("",0.0)
        self.shook = False

    def onUnload(self):
        self._awareness.setEnabled(True)
        self.autonomouslife.setAutonomousAbilityEnabled("BasicAwareness", True)
        self.autonomouslife.setAutonomousAbilityEnabled("BackgroundMovement", True)

    def onInput_onStart(self):
        time.sleep(0.5)

        currentPOS = self.motion.getAngles(self.joints, True)[:-3]
        if self.comparePos(currentPOS, self.fAngles[:-3], 0.15):
            deltas = []
            movedJoints = []
            self.fAngles = self.motion.getAngles(self.joints, True)
            self.logger.info(self.fAngles)
            for i in range(25):
                currentPOS = self.motion.getAngles(self.joints, True)[:-3]
                if not self.comparePos(currentPOS, self.fAngles[:-3], 0.15):
                    self.onShaken()
                    self.shook = True
                    break
                time.sleep(0.2)
        else:
            deltas = []
            movedJoints = []
            self.fAngles = self.motion.getAngles(self.joints, True)
            self.logger.info(self.fAngles)
            for i in range(25):
                currentPOS = self.motion.getAngles(self.joints, True)[:-3]
                if not self.comparePos(currentPOS, self.fAngles[:-3], 0.15):
                    self.onShaken()
                    self.shook = True
                    break
                time.sleep(0.2)
        self.logger.info(self.fAngles)
        self.logger.info(currentPOS)
        self.logger.info("Sale")
        if not self.shook:
            self.onStopped()

    def comparePos(self, currentPOS, targetPOS, threshold):
        currentPOS = np.array(currentPOS)
        targetPOS = np.array(targetPOS)
        diff = np.abs(currentPOS - targetPOS)/ targetPOS
        diff = np.abs(diff)

        result = True
        if np.max(diff) > threshold:
            result = False
            self.movedJoint = (self.joints[np.argmax(diff)], np.max(diff))

        return result

    def onInput_onStop(self):
        self.onUnload()]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="4" /><Output name="onShaken" type="1" type_size="1" nature="2" inner="0" tooltip="" id="5" /></Box><Box name="Apagar Auto" id="29" localization="8" tooltip="" x="224" y="26"><bitmap>media/images/box/box-python-script.png</bitmap><script language="4"><content><![CDATA[class MyClass(GeneratedClass):
