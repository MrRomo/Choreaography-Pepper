        sText = "¡Qué bien luces! <br><br>Gracias por interactuar conmigo. <br><br>Espero que volvamos a vernos pronto, <br><br><br> <i>" + self.robotName + "</i>"
        sAttachedFilePath = "/home/nao/.local/share/PackageManager/apps/TakePicture/html/image.jpg"
        sPwd = "CdCBancolombia1*"
        sSmtp = "smtp.gmail.com"
        sPort = 587
        try:
            sPort = int( sPort )
            bValidPort = ( sPort >= 0 and sPort <= 65535 )
        except:
            bValidPort = False
        if( not bValidPort ):
            raise Exception( str(sPort) + " is not a valid port number to use to send e-mail. It must be an integer between 0 and 65535. Please check that the port parameter of the box is correct." )
