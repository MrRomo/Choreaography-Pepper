            time.sleep(0.5)
            self.leds.fadeRGB("FaceLeds", 1, 1, 1, 0)
            self.say("O así, ")
            self.say("Indica que estoy procesando, y no podré comprender lo que me dices. ")
            time.sleep(1)
            self.teach = False
            self.say("Continuémos: ")

        self.oText(self.text)
        self.onUnload()

    def onInput_iFlag(self):
        self.teach = False

    def say(self, t):
        self.bIsRunning = True
        try:
            movement = "contextual"
            sentence = "\RSPD="+ "100" + "\ "
            sentence += "\VCT="+ "100" + "\ "
            sentence += t
            sentence +=  "\RST\ "
            id = self.tts.post.say(str(sentence), {"speakingMovementMode":movement})
            self.ids.append(id)
            self.tts.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.bIsRunning = False]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="iText" type="3" type_size="1" nature="1" inner="0" tooltip="" id="2" /><Input name="iFlag" type="1" type_size="1" nature="1" inner="0" tooltip="" id="3" /><Output name="oText" type="0" type_size="1" nature="2" inner="0" tooltip="" id="4" /></Box><Box name="Condicion Nombre" id="6" localization="8" tooltip="Test a condition and stimulate the then or else outputs depending on the boolean&#x0A;value of the condition.&#x0A;The condition can be set in parameters." x="946" y="222"><bitmap>media/images/box/box-diagram.png</bitmap><script language="4"><content><![CDATA[class MyClass(GeneratedClass):
