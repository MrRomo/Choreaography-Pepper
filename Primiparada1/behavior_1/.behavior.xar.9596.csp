        self.teach = False

    def onLoad(self):
        self.bIsRunning = False
        self.ids = []
        self.text = ""

    def onUnload(self):
        for id in self.ids:
            try:
                self.ttsStop.stop(id)
            except:
                pass
        while( self.bIsRunning ):
            time.sleep( 0.2 )

        self.text = ""

    def onInput_iText(self, p):
        self.text = p

        if self.teach:
            self.say("Al interactuar conmigo, ten presente lo que representa el color de mis ojos: ")
            self.say("Esto indica que puedes hablarme, ")
            self.leds.rotateEyes(0x002D50FF, 1, 0.8)
